<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity19">


    <TextView
        android:id="@+id/catergorytext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:text="Category"
        android:textSize="26sp"
        android:textStyle="bold"
        android:textColor="#37474F"
        android:clickable="true"
        android:focusable="true" />


    <ImageView
        android:id="@+id/dish1View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/dish1" />

    <ImageView
        android:id="@+id/dish2View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/dish2" />

    <ImageView
        android:id="@+id/dish3View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="370dp"
        android:src="@drawable/dish3" />

    <ImageView
        android:id="@+id/dish4View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="370dp"
        android:src="@drawable/dish4" />

    <ImageView
        android:id="@+id/dish5View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="540dp"
        android:src="@drawable/dish5" />

    <ImageView
        android:id="@+id/dish6View"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="540dp"
        android:src="@drawable/dish6" />



    <ImageView
        android:id="@+id/homenavigationView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="720dp"
        android:src="@drawable/homenavigation" />

    <ImageView
        android:id="@+id/toolnavigationView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="720dp"
        android:src="@drawable/toolnavigation" />

    <ImageView
        android:id="@+id/shopnavigationView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="720dp"
        android:src="@drawable/shopnavigation" />

    <ImageView
        android:id="@+id/linenavigationView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="330dp"
        android:layout_marginTop="720dp"
        android:src="@drawable/linenavigation" />

</RelativeLayout>






